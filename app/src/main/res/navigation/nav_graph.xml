<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.github.htdangkhoa.nexterp.ui.main.fragments.home.index.HomeIndexFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/nav_receiving"
        android:name="com.github.htdangkhoa.nexterp.ui.main.fragments.receiving.list.ReceivingListFragment"
        android:label="@string/menu_receiving"
        tools:layout="@layout/fragment_receiving_list" >
        <action
            android:id="@+id/action_nav_receiving_to_receivingFormFragment"
            app:destination="@id/receivingFormFragment" />
    </fragment>
    <fragment
        android:id="@+id/receivingFormFragment"
        android:name="com.github.htdangkhoa.nexterp.ui.main.fragments.receiving.form.ReceivingFormFragment"
        android:label="fragment_receiving_form"
        tools:layout="@layout/fragment_receiving_form" >
        <argument
            android:name="id"
            app:argType="integer" />
    </fragment>
</navigation>